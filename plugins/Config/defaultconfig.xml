<config>
	<groupchat>
		<window>
			<config path="/groupchat/window">
				<config>
					<color label="Color">
						<forreign_text data='#00CC00' label='Other&apos;s text color' type='string'></forreign_text>
						<forreign_name data='#000000' label='Other&apos;s nick color' type='string'></forreign_name>
						<my_name data='#888888' label='My nick color' type='string'></my_name>
						<my_text data='#AAAAAA' label='My text color' type='string'></my_text>
						<highlight data='#0000FF' label='Highlight color' type='string'></highlight>
						<notice data='#000044' label='Notice color' type='string'></notice>
						<timestamp data='#BBBBBB' label='Timestamp color' type='string'></timestamp>
					</color>
					<nickcompletion label='Nick compleation'>
						<separator label='What to write after nick completion' type='text'>, </separator>
					</nickcompletion>
					<textarea height='1' width='1'></textarea>
					<timestamp label="Timestamp">
						<format label='Timestamp Format' type='text'>[%H:%M]</format>
					</timestamp>
				</config>
			</config>
		</window>
		
		<joinwindow>
			<config path='/groupchat/joinwindow'>
				<config>
					<server data='conference.jabber.org'></server>
					<room data='jabber'></room>
					<nick data=''></nick>
				</config>
			</config>
		</joinwindow>
	</groupchat>
	<chat>
		<window>
			<config path='/chat/window'><config>
					<display type='' label="Display">
						<myname type='' label="My nick">
							<show data='false' label='To show your own nick' type='bool'></show>
							<pre label='Before my nick' type='text'></pre>
							<post label='After my nick' type='text'>> </post>
							<pre_me label='Before my nick with /me' type='text'></pre_me>
							<post_me label='After my nick with /me' type='text'>* </post_me>
							<continuing label='Second marker' type='text'></continuing>
						</myname>
						<forreignname type='' label="Other&apos;s nick">
							<pre label='Before other&apos;s nick' type='text'></pre>
							<post label='Before other&apos;s nick' type='text'>> </post>
							<pre_me label='Before other&apos;s nick with /me' type='text'></pre_me>
							<post_me label='After other&apos;s nick with /me' type='text'>* </post_me>
							<continuing label='Second marker' type='text'></continuing>
						</forreignname>
					</display>
					<timestamp type='' label="Timestamp">
						<format label='Timestamp format' type='text'>[%H:%M]</format>
					</timestamp>
					<color type='' label="Color">
						<my_name data='#888888' label='My nick color' type='string'></my_name>
						<my_text data='#AAAAAA' label='My text color' type='string'></my_text>
						<forreign_name data='#00CC00' label='Other&apos;s nick color' type='string'></forreign_name>
						<forreign_text data='#000000' label='Other&apos;s text color' type='string'></forreign_text>
						<timestamp data='#BBBBBB' label='Timestamp color' type='string'></timestamp>
					</color>
					<window_spec input_area_height='49' type=''></window_spec>
					<jidbar type='' label="JIDBar">
						<show data='true' label='Show Jid Bar' type='bool'></show>
					</jidbar>
				</config></config></window></chat>
				
